{{#members}}
<div class="member clearfix" data-id="{{userId}}">
  <div class="pull-left">
    <img class="member-icon" src="{{photo}}" alt="" width="45" height="45" data-icon-text="{{name}}">
  </div>

  <div class="text">
    <div class="name">{{name}}</div>
    <div class="link"><a href="#"{{email}}</a></div>
  </div>
  <div class="clearfix pull-right actions">
    <div class="pull-left link2">
      <!--<a href="manage-member.html?id={{userId}}">Manage</a>-->
    </div>
    <div class="remove-user pull-left"><a href="#"><i class="ico ico-cross"></i></a></div>
  </div>
</div>
{{/members}}
